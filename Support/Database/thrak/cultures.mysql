DROP TABLE IF EXISTS  /* <xql expand="table_prefix"> */cultures ;

CREATE TABLE thrak./* <xql expand="table_prefix"> */cultures
   (
                cul_id					INT			NOT NULL AUTO_INCREMENT
										COMMENT 'Unique id for this culture',
                cul_name                                CHAR(16)                NOT NULL DEFAULT ''
                                                                                COMMENT 'Culture name',
                cul_country_alpha2_code                 CHAR(2)                 NOT NULL DEFAULT ''
                                                                                COMMENT 'Corresponding country alpha-2 code (ISO 3361) when possible',
                cul_description                         CHAR(64)                NOT NULL DEFAULT ''
                                                                                COMMENT 'Culture description',

                PRIMARY KEY    pk_cultures_by_id                                ( cul_id ),
                KEY            ix_cultures_by_name                              ( cul_name ),
                KEY            ix_cultures_by_alpha2                            ( cul_country_alpha2_code, cul_name )
    )  ENGINE = MyISAM DEFAULT CHARSET latin1 ;


